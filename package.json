{
  "name": "mithril-level-editor",
  "version": "0.0.1",
  "description": "A simple level editor using Mithril.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pixelpicosean/mithril-level-editor.git"
  },
  "keywords": [
    "editor",
    "game"
  ],
  "author": "Sean Bohan",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pixelpicosean/mithril-level-editor/issues"
  },
  "homepage": "https://github.com/pixelpicosean/mithril-level-editor#README.md",
  "devDependencies": {
    "babelify": "^6.1.2",
    "browser-sync": "^2.7.6",
    "browserify": "^10.2.3",
    "browserify-shim": "^3.8.7",
    "del": "^1.2.0",
    "gulp": "^3.8.11",
    "gulp-concat": "^2.5.2",
    "gulp-sass": "^2.0.1",
    "gulp-sourcemaps": "^1.5.2",
    "gulp-uglify": "^1.2.0",
    "gulp-util": "^3.0.5",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0"
  },
  "browserify": {
     "transform": [
       "browserify-shim"
     ]
   },
   "browserify-shim": {
     "mithril": "global:m",
     "kefir": "global:Kefir",
     "pixi": "global:PIXI",
     "mousetrap": "global:Mousetrap"
   }
}
